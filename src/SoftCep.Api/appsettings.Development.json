{
  "ConnectionStrings": {
    "Redis": "192.168.1.234:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Infrastructure": {
    "ViaCep": {
      "BaseUrl": "https://viacep.com.br/ws/"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "Using": [
      "Serilog.Enrichers.ClientInfo"
    ],
    "Enrich": [
      "WithClientIp",
      "WithCorrelationId",
      "FromLogContext",
      "WithProcessId",
      "WithThreadId",
      {
        "Name": "WithRequestHeader",
        "Args": {
          "headerName": "User-Agent"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
